{
  "name": "dofus-sets",
  "version": "1.0.0",
  "description": "A better set builder for the MMORPG Dofus",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.server.json server",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .build/server",
    "type-check": "tsc",
    "apollo-codegen": "apollo client:codegen --target typescript --endpoint=http://localhost:5000/graphql",
    "publish-heroku": "cd ../ && git subtree push --prefix client heroku-client master || true",
    "deploy-production": "pm2 deploy ecosystem.config.js production --force",
    "reload-production": "pm2 deploy ecosystem.config.js production exec 'pm2 reload app'"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@react-hook/debounce": "^1.0.11",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.144",
    "@zeit/next-css": "^1.0.1",
    "antd": "^4.0.0",
    "apollo-boost": "^0.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "i18next": "^19.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "^9.2.2",
    "next-i18next": "^4.2.0",
    "next-with-apollo": "^4.3.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-waypoint": "^9.0.2",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "apollo": "^2.21.0",
    "babel-plugin-import": "^1.12.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
}
