{
  "name": "dofuslab",
  "version": "1.0.0",
  "description": "An equipment experiment tool for the MMORPG Dofus",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "server/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.server.json server",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .build/server",
    "type-check": "tsc",
    "apollo-codegen": "apollo client:codegen --target typescript --endpoint=http://localhost:5000/api/graphql",
    "publish-heroku": "cd ../ && git subtree push --prefix client heroku-client master || true",
    "deploy-production": "pm2 deploy ecosystem.config.js production --force",
    "reload-production": "pm2 deploy ecosystem.config.js production exec 'pm2 reload app'",
    "sync-i18n": "ts-node --project tsconfig.server.json sync-locales",
    "scan-i18n": "npx i18next-scanner"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^2.0.2",
    "@ant-design/icons": "^4.1.0",
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@artsy/fresnel": "^1.1.0",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.22",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@next/bundle-analyzer": "^9.4.2",
    "@react-hook/debounce": "^2.0.5",
    "@zeit/next-css": "^1.0.1",
    "antd": "^4.2.5",
    "apollo": "^2.28.0",
    "apollo-boost": "^0.4.9",
    "babel-plugin-import": "^1.13.0",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "graphql": "14.6.0",
    "i18next": "^19.4.5",
    "less": "^3.11.1",
    "less-vars-to-js": "^1.3.0",
    "lockr": "^0.8.5",
    "lodash": "^4.17.15",
    "next": "^9.4.2",
    "next-fonts": "^1.1.0",
    "next-i18next": "^4.4.2",
    "next-with-apollo": "^5.0.1",
    "null-loader": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "react-no-ssr": "^1.1.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/gtag.js": "^0.0.3",
    "@types/lockr": "^0.8.6",
    "@types/lodash": "^4.14.152",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-no-ssr": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "@zeit/next-less": "^1.0.1",
    "babel-plugin-inline-import": "^3.0.0",
    "chalk": "^4.0.0",
    "i18next-scanner": "^2.11.0",
    "nodemon": "^2.0.4",
    "sort-keys": "^4.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
